            Call Subroutine(player_1SecondCooldown);
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[5] && Event Player.player_DeathPenalty_Enabled == False);
            Event Player.player_DeathPenalty_Enabled = True;
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[6] && Event Player.player_MirrorMode_Enabled == False);
            Event Player.player_MirrorMode_Enabled = True;
