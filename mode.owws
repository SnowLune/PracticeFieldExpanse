		Subroutine;
		player_SetDifficulty;
	}

	actions
	{
        Players In Slot(Slot Of(Event Player), Team 1).player_DifficultyName = Global.difficultyNames[Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty];
        Players In Slot(Slot Of(Event Player), Team 2).botAI_Facing_Base = 1 + 0.01 * Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty;
        Players In Slot(Slot Of(Event Player), Team 2).botAI_FacingPadMin = 2.5 * Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty;
        Players In Slot(Slot Of(Event Player), Team 2).botAI_FacingPadMax = 5 * Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty;
        Players In Slot(Slot Of(Event Player), Team 2).botAI_FacingCapMin = 90 * Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty;
        Players In Slot(Slot Of(Event Player), Team 2).botAI_FacingCapMax = 135 * Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty;
        Players In Slot(Slot Of(Event Player), Team 2).botAI_FacingAngleModifier = 0.5 * Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty;
        Players In Slot(Slot Of(Event Player), Team 2).botAI_MoveWASD_TimeMod = 1 - 0.1 * Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty * 0.55 + 0.15;
        Players In Slot(Slot Of(Event Player), Team 2).botAI_MoveCrouch_TimeMod = 0.08 * Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty;
        Players In Slot(Slot Of(Event Player), Team 2).botAI_MoveCrouch_ChanceMod = 0.1 + Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty * 0.01;
        Players In Slot(Slot Of(Event Player), Team 2).botAI_MoveJump_TimeMod = 1 + Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty * 0.07;
        Players In Slot(Slot Of(Event Player), Team 2).botAI_MoveJump_ChanceMod = 0.16 + Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty * 0.03;
        Players In Slot(Slot Of(Event Player), Team 2).botAI_ReactionTime = Absolute Value(0.08 * Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty - 0.87);
        Players In Slot(Slot Of(Event Player), Team 1).player_SelfHealAmount = Absolute Value(Players In Slot(Slot Of(Event Player), Team 1).player_Difficulty - 9.5) * 0.1;
		Small Message(Players In Slot(Slot Of(Event Player), Team 1), Custom String("Difficulty Set To {0}", Players In Slot(Slot Of(
			Event Player), Team 1).player_DifficultyName));
	}
}

rule("Subroutine: cycleHeroType")
{
	event
	{
		Subroutine;
		cycleHeroType;
	}

	actions
	{
		If(Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroListName == Global.heroClassName_All);
			Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroList = All Tank Heroes;
			Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroListName = Global.heroClassName_Tank;
		Else If(Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroListName == Global.heroClassName_Tank);
			Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroList = All Damage Heroes;
			Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroListName = Global.heroClassName_Damage;
		Else If(Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroListName == Global.heroClassName_Damage);
			Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroList = All Support Heroes;
			Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroListName = Global.heroClassName_Support;
		Else If(Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroListName == Global.heroClassName_Support);
			Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroList = All Heroes;
			Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroListName = Global.heroClassName_All;
		End;
        If(Players In Slot(Slot Of(Event Player), Team 1).player_HeroListShuffled == True);
            Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroList = Randomized Array(Players In Slot(Slot Of(Event Player), Team 1).player_CurrentHeroList);
        End;
		Call Subroutine(resetProgress);
	}
}

