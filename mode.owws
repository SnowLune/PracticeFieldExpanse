rule("Player Sky Menu Arrow Visibility")
{
	event
	{
		Subroutine;
        skyMenuArrowVisibility;
	}

	actions
	{
        If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[0]);
            If(Event Player.player_Difficulty == 0);
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Null;
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Event Player;
            Else If(Event Player.player_Difficulty == 9);
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Event Player;
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Null;
            Else;
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Event Player;
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Event Player;
            End;
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[1]);
            If(Event Player.player_KillQuota == 2);
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Null;
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Event Player;
            Else If(Event Player.player_KillQuota == 50);
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Event Player;
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Null;
            Else;
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Event Player;
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Event Player;
            End;
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[2]);
            Event Player.player_SkyMenuArrowsVisibleTo[0] = Null;
            Event Player.player_SkyMenuArrowsVisibleTo[1] = Event Player;
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[4]);
            If(Event Player.player_1SecondCooldown_Enabled == False);
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Null;
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Event Player;
            Else If(Event Player.player_1SecondCooldown_Enabled == True);
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Null;
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Event Player;
            End;
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[5]);
            If(Event Player.player_DeathPenalty_Enabled == False);
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Null;
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Event Player;
            Else If(Event Player.player_DeathPenalty_Enabled == True);
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Null;
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Event Player;
            End;
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[6]);
            If(Event Player.player_MirrorMode_Enabled == False);
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Null;
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Event Player;
            Else If(Event Player.player_MirrorMode_Enabled == True);
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Null;
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Event Player;
            End;
		Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[7]);
            If(Players In Slot(Slot Of(Event Player), Team 2).healing_Enabled == False);
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Null;
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Event Player;
            Else If(Players In Slot(Slot Of(Event Player), Team 2).healing_Enabled == True);
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Null;
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Event Player;
            End;
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[8]);
            If(Event Player.healing_Enabled == False);
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Null;
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Event Player;
            Else If(Event Player.healing_Enabled == True);
                Event Player.player_SkyMenuArrowsVisibleTo[1] = Null;
                Event Player.player_SkyMenuArrowsVisibleTo[0] = Event Player;
            End;
        Else;
            Event Player.player_SkyMenuArrowsVisibleTo[0] = Event Player;
            Event Player.player_SkyMenuArrowsVisibleTo[1] = Event Player;
        End;
	}
}

