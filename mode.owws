rule("Player Sky Menu Toggle")
{
	event
	{
        Ongoing - Each Player;
		Team 1;
		All;
	}

    conditions
	{
		Is Button Held(Event Player, Melee) == True;
        Is Button Held(Event Player, Reload) == True;
	}

	actions
	{
        Wait(0.25, Abort When False);
        If(Event Player.player_SkyMenu_isOpen == False);
            If(Players In Slot(Slot Of(Event Player), Team 2).botAI_Enabled == True);
                Call Subroutine(botAI_AIMode_Disable);
            End;
            Set Status(Event Player, Null, Phased Out, 9999);
            Start Forcing Player Position(Event Player, Event Player.player_Home, False);
	        Start Facing(Event Player, Facing Direction Of(Event Player), 100, To World, Direction and Turn Rate);
            Disallow Button(Event Player, Primary Fire);
		    Disallow Button(Event Player, Secondary Fire);
            Disallow Button(Event Player, Ability 1);
            Disallow Button(Event Player, Ability 2);
            Disallow Button(Event Player, Ultimate);
            Disallow Button(Event Player, Reload);
            Disallow Button(Event Player, Melee);
            Skip If(Health(Event Player) > Max Health(Event Player) / 4, 1);
            Heal(Event Player, Null, 75);
            Start Camera(Event Player, Event Player.player_Home + Vector(0, 30, 0), Event Player.player_SkyMenu_Selection, 30);
            Event Player.player_SkyMenuVisibleTo = Event Player;
            Call Subroutine(skyMenuArrowVisibility);
            Event Player.player_SkyMenu_isOpen = True;
            Big Message(Event Player, Custom String("Use WASD to navigate the menu."));
            Small Message(Event Player, Custom String("Use WASD to navigate the menu."));
        Else;
            Clear Status(Event Player, Phased Out);
            Stop Forcing Player Position(Event Player);
            Stop Facing(Event Player);
            Allow Button(Event Player, Primary Fire);
		    Allow Button(Event Player, Secondary Fire);
            Allow Button(Event Player, Ability 1);
            Allow Button(Event Player, Ability 2);
            Allow Button(Event Player, Ultimate);
            Allow Button(Event Player, Reload);
            Allow Button(Event Player, Melee);
            Stop Camera(Event Player);
            Event Player.player_SkyMenu_isOpen = False;
            Wait(0.1, Ignore Condition);
            Event Player.player_SkyMenuVisibleTo = Null;
            Event Player.player_SkyMenuArrowsVisibleTo[0] = Null;
            Event Player.player_SkyMenuArrowsVisibleTo[1] = Null;
        End;
        Play Effect(Event Player, Explosion Sound, White, Event Player, 45);
	}
}

