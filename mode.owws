rule("Player Sky Menu Move Selection Decrease")
{
	event
	{
		Ongoing - Each Player;
		Team 1;
		All;
	}

	conditions
	{
        Event Player.player_SkyMenu_isOpen == True;
		Throttle Of(Event Player) == Left;
	}

	actions
	{
        Play Effect(Event Player, Explosion Sound, White, Event Player, 15);
        "Difficulty"
        If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[0] && Event Player.player_Difficulty > 0);
		    Event Player.player_Difficulty -= 1;
		    Call Subroutine(player_SetDifficulty);
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[1] && Event Player.player_KillQuota > 2);
            Event Player.player_KillQuota -= 2;
		    Event Player.currentKillNumber = 0;
		    Call Subroutine(setEnemyDistanceModifier);
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[3]);
            Call Subroutine(shuffleHeroList);
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[4]);
            Call Subroutine(player_1SecondCooldown);
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[5] && Event Player.player_DeathPenalty_Enabled == True);
            Event Player.player_DeathPenalty_Enabled = False;
        Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[6] && Event Player.player_MirrorMode_Enabled == True);
            Event Player.player_MirrorMode_Enabled = False;
		Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[7] && Players In Slot(Slot Of(Event Player), Team 2).healing_Enabled == True);
            Players In Slot(Slot Of(Event Player), Team 2).healing_Enabled = False;
		Else If(Event Player.player_SkyMenu_Selection == Event Player.player_SkyMenu[8] && Event Player.healing_Enabled == True);
            Event Player.healing_Enabled = False;
        End;
        Call Subroutine(skyMenuArrowVisibility);
	}
}

rule("Player Sky Menu Move Selection Increase")
